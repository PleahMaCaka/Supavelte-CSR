{
  "name": "supavelte",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build-storybook": "storybook build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev --host",
    "init": "pnpm i && cp .env.example .env && pnpm init:android",
    "init:android": "pnpm tauri android init && cd ./src-tauri/gen/android && ./gradlew --refresh-dependencies && pnpm init:android:properties",
    "init:android:properties": "PTS_PATH=\"./src-tauri/gen/android/local.properties\" && touch $PTS_PATH && echo \"sdk.dir=$ANDROID_HOME\" > $PTS_PATH",
    "lint": "eslint . --fix",
    "prepare": "husky",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "supabase": "supabase",
    "tauri": "tauri",
    "tauri:icon": "tauri icon static/favicon.png",
    "test": "pnpm test:vitest",
    "test:supabase": "supabase test db",
    "test:vitest": "vitest",
    "vitest": "vitest",
    "typegen": "supabase gen types typescript --linked > src/lib/types/gen/supabase.d.ts"
  },
  "lint-staged": {
    "src/**": [
      "eslint"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@iconify/svelte": "^4.0.2",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/addon-interactions": "^8.3.5",
    "@storybook/addon-links": "^8.3.5",
    "@storybook/addon-svelte-csf": "^4.1.7",
    "@storybook/blocks": "^8.3.5",
    "@storybook/svelte": "^8.3.5",
    "@storybook/sveltekit": "^8.3.5",
    "@storybook/test": "^8.3.5",
    "@supabase/supabase-js": "^2.45.4",
    "@sveltejs/adapter-static": "^3.0.5",
    "@sveltejs/kit": "^2.6.4",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@tauri-apps/api": "^2.0.2",
    "@tauri-apps/cli": "2.0.2",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.13",
    "dotenv": "^16.4.5",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-storybook": "^0.9.0",
    "eslint-plugin-svelte": "^2.44.1",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "pretendard": "^1.3.9",
    "process": "^0.11.10",
    "sass": "^1.79.5",
    "storybook": "^8.3.5",
    "svelte": "^4.2.19",
    "svelte-check": "^4.0.4",
    "svelte-eslint-parser": "^0.41.1",
    "tailwindcss": "^3.4.13",
    "tslib": "^2.7.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vitest": "^2.1.2"
  },
  "packageManager": "pnpm@9.15.9",
  "pnpm": {
    "overrides": {
      "whatwg-url": "^14.0.0"
    }
  }
}
