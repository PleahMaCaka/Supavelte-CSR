{
  "name": "supavelte",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "pnpm test:vitest test:supabase",
    "test:vitest": "vitest",
    "test:supabase": "supabase test db",
    "lint": "eslint .",
    "typegen": "supabase gen types typescript --linked > src/lib/types/gen/supabase.d.ts",
    "init": "pnpm i && cp .env.example .env && pnpm init:android",
    "init:android": "pnpm tauri android init && cd ./src-tauri/gen/android && ./gradlew --refresh-dependencies && pnpm init:android:properties",
    "init:android:properties": "PTS_PATH=\"./src-tauri/gen/android/local.properties\" && touch $PTS_PATH && echo \"sdk.dir=$ANDROID_HOME\" > $PTS_PATH",
    "tauri": "tauri",
    "tauri:icon": "tauri icon static/favicon.png",
    "supabase": "supabase",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.6.0",
    "@supabase/supabase-js": "^2.44.3",
    "@sveltejs/adapter-static": "^3.0.2",
    "@sveltejs/kit": "^2.5.18",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@tauri-apps/api": "^2.0.0-beta",
    "@tauri-apps/cli": "^2.0.0-beta",
    "@types/eslint": "^8.56.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.12.10",
    "dotenv": "^16.4.5",
    "eslint": "^9.6.0",
    "eslint-plugin-svelte": "^2.41.0",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "internal-ip": "^8.0.0",
    "postcss": "^8.4.39",
    "process": "^0.11.10",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.4",
    "svelte-eslint-parser": "^0.39.2",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vitest": "^2.0.1"
  },
  "packageManager": "pnpm@9.4.0",
  "pnpm": {
    "overrides": {
      "whatwg-url": "^14.0.0"
    }
  }
}
